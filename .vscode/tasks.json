{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "build ui",
			"type": "shell",
			"command": "make clean assets",
			"options": {
				"cwd": "${workspaceFolder}"
			},
			"problemMatcher": [],
			"group": {
				"kind": "build",
				"isDefault": true
			}
		},
		{
			"label": "build ui (dev)",
			"type": "shell",
			"command": "yarn run dev",
			"options": {
				"cwd": "${workspaceFolder}/ui"
			},
			"problemMatcher": [],
			"group": "build",
		}
	]
}